{
  "extends": [
    "airbnb",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
},
"rules":{
  "linebreak-style": 0,
  "jsx-a11y/label-has-associated-control": ["error", {
    "required": {
      "some": ["nesting", "id"]
    }
  }],
  "jsx-a11y/label-has-for": ["error", {
    "required": {
      "some": ["nesting", "id"]
    }
  }],
  "import/prefer-default-export": 0
}
}
